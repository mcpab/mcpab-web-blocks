{
  "name": "@mcpab/web-blocks",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format:write": "prettier -w \"src/**/*.{ts,tsx,md,json}\"",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "test": "jest"
  },
  "peerDependencies": {
    "@mui/icons-material": ">=6",
    "@mui/material": ">=6",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@ladle/react": "^5.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9",
    "jest": "^30.2.0",
    "prettier": "^3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/system": "^7.3.5"
  }
}