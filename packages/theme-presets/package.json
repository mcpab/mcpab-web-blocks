{
  "name": "@mcpab/theme-presets",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mcpab/mui-theme": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/material": ">=5"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^6.0.0",
       "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7"
  },
   "pnpm": {
    "overrides": {
      "@types/react": "18.3.27",
      "@types/react-dom": "18.3.7",
      "@mui/material": "7.3.6",
      "@emotion/react": "11.14.0",
      "@emotion/styled": "11.14.0"
    }
  }
}
